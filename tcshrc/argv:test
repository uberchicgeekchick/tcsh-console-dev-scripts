#!/bin/tcsh
if(! ${?TCSH_RC_SESSION_PATH} ) setenv TCSH_RC_SESSION_PATH "/projects/cli/console.pallet/tcshrc";

set scripts_name=".cshrc.tcsh";
source "${TCSH_RC_SESSION_PATH}/argv:check" "${scripts_name}" ${argv};

if( $args_handled > 0 ) then
	@ args_shifted=0;
	while( $args_shifted < $args_handled )
		@ args_shifted++;
		shift;
	end
	unset args_shifted;
endif
unset args_handled;



source "${TCSH_RC_SESSION_PATH}/argv:clean-up" "${scripts_name}";
unset scripts_name;
