#!/bin/tcsh -f
source /profile.d/tcshrc/environment.tcsh
source /profile.d/tcshrc/aliases:sudo.tcsh
source /profile.d/tcshrc/programs.tcsh
source /profile.d/tcshrc/sshfs:mount:ocs.tcsh

foreach rc_file ( /profile.d/tcshrc/*.tcsh )
	switch("${rc_file}")
	case "bindkey.tcsh":
	case "complete.tcsh":
	case "aliases:sudo.tcsh":
	case "environment.tcsh":
	case "programs.tcsh":
	case "sshfs:mount:ocs.tcsh":
		breaksw
	default:
		source "${rc_file}"
		if( ${?http_proxy} ) unsetenv http_proxy
		breaksw
	endsw
end
unset rc_file

